## Copy this file to .env and fill your credentials
## IMPORTANT: Never commit your .env to version control.

# Wealthsimple login
WS_USERNAME=your.email@example.com
WS_PASSWORD=replace-with-strong-password
# Optional: OTP value (only if prompted this run; otherwise leave empty)
WS_OTP=

# Telegram bot (optional)
TELEGRAM_BOT_TOKEN=replace-with-token
TELEGRAM_CHAT_ID=replace-with-chat-id

# Enhanced AI Advisor (optional)
AI_ENHANCED=0

# Cache tuning (optional)
CACHE_DB_PATH=cache.sqlite3
CACHE_TTL_QUOTE_SEC=60
CACHE_TTL_SERIES_SEC=300
CACHE_TTL_NEWS_SEC=600
